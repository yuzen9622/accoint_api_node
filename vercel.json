{
    "version": 2,
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ],
    "crons": [
        {
            "path": "/api/accountCron",
            "schedule": "0 0 * * *"
        }
    ]
}